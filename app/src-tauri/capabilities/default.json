{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "enables the default permissions",
	"windows": ["main"],
	"permissions": [
		"core:default",
		"core:path:default",
		"dialog:default",
		"shell:default",
		"fs:allow-appdata-read-recursive",
		"fs:allow-appdata-write-recursive",
		"fs:allow-mkdir",
		"fs:allow-write-file",
		"fs:allow-read-file",
		"fs:allow-home-read-recursive"
	]
}
